server {
    listen 80;
    server_tokens off;
    # ip address or domain
    # server_name www.mysite.com mysite.com
    server_name 104.197.13.112;

     location / {
         include uwsgi_params;
         uwsgi_pass unix:/var/www/flasktest/flasktest.sock;
     }

     location /static {
         alias /var/www/flasktest/static;
     }
}